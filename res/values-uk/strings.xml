    <string name="play_all">"Відтворити усі"</string>

    <!-- Delete confirmation dialog when deleting an entire artist -->
    <string name="delete_artist_desc">"Всі композиції виконавця <xliff:g id="artist">%s</xliff:g> будуть видалені з SD-карти."</string>

